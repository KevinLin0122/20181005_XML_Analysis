@model List<HC.Models.OpenData>
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table">
<thead>
    <tr>
        <th>companyname</th>
        <th>Category</th>

    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.companyname</td>
            <td>@item.Category</td>
        </tr>
    }


</tbody>



</table>