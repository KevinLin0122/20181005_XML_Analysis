@model List<HC.Models.OpenData>
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table">
<thead>
    <tr>
        <th>companyname</th>
        <th>Category</th>
        <th>詳細</th>

    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.companyname</td>
        <td>@item.Category</td>
        <td>
            <a type="button" class="btn btn-warning" href="/OpenData/Detail/@item.id">詳細</a>
        </td>
    </tr>
    }


</tbody>



</table>